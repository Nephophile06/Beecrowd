#include <stdio.h>

int main()
{
    float X, sum = 0;
    int count = 0;

    while (count != 2)
    {
        scanf("%f", &X);

        if ((X >= 0) && (X <= 10))
        {
            count++;
            sum = sum + X;   
        }
        else
            printf("nota invalida\n");
    }

    float avg = sum / 2.0;
    printf("media = %.2f\n", avg);

    return 0;
}
