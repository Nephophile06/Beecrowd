#include <stdio.h>

int main()
{
    float a, sum = 0, avg;
    int count = 0;

    for (int i = 0; i < 6; i++)
    {
        scanf("%f", &a);
        if (a > 0)
        {
            count++;
            sum = sum + a;
            avg = sum / count;
        }
    }
    printf("%d valores positivos\n", count);
    printf("%.1f\n", avg);

    return 0;
}
