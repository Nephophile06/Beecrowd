#include <stdio.h>

int main()
{
    int N;
    int count = 0, sum = 0;
    float avg;

    while (1)
    {
        scanf("%d", &N);

        if (N >= 0)
            count++;
        else 
            break;
        sum = sum + N;

        avg = (float)sum / (float)count;
    }

    printf("%.2f\n", avg);

    return 0;
}
