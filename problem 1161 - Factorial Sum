#include <stdio.h>

int main()
{
    long long int M, N;
    scanf("%lld %lld", &M, &N);
    long long int count1 = 1, count2 = 1; 

    for (int i = 1; i <= M; i++)
    {
        count1 = i * count1;
    }

    for (int j = 1; j <= N; j++)
    {
        count2 = j * count2;
    }

    long long int count = count1 + count2;
    printf("%lld\n", count);

    return 0;
}
