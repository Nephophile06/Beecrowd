#include <stdio.h>

int main()
{
    int a, b, c;
    int temp;
    scanf("%d %d %d", &a, &b, &c);
    int A, B, C;
    A = a; B = b; C = c;
        if (a > c)
        {
           temp = a;
           a = c;
           c = temp;
        }
        if (a > b)
        {
            temp = a;
            a = b;
            b = temp;
        }
        if (b > c)
        {
            temp = b;
            b = c;
            c = temp;
        }
    printf("%d\n%d\n%d\n", a, b, c);
    printf("\n");
    printf("%d\n%d\n%d\n", A, B, C);

    return 0;
}
