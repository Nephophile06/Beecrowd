#include <stdio.h>

int main()
{
    int N;
    scanf("%d", &N);
    float a, b, c;

    for (int i = 0; i < N; i++)
    {
        scanf("%f %f %f", &a, &b, &c);
        int sum = 2 + 3 + 5;
        float avg = ((a*2) + (b*3) + (c*5)) / sum;
        printf("%.1f\n", avg);
    }         

    return 0;
}
